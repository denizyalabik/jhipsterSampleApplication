entity QuestionAnswer {
    question String required,
  	correctAnswer String required,
    wrongAnswers String required
}

entity User{
	Name String,
    Surname String,
	UserName String,
  	email String,
    password String,
    uRole Role 
}

enum Role{
	ADMIN , TEACHER , STUDENT
}

relationship OneToMany {
	User {questionanswer} to QuestionAnswer{}

}

paginate QuestionAnswer, User with pagination
dto * with mapstruct
service all with serviceImpl
